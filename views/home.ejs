<% include partials/header %>

<header>
    <div class="bg-blue collapse" id="navbarHeader">
      <div class="container">
        <div class="row">
          <div class="col-sm-8 col-md-7 py-4">
            <p class="text-light">This is an open source Node + Express application to track a daily activity of your choice, such as coding, exercising, reading, or anything you like, inspired by the GitHub contribution graph and powered by Google Sheets. Feel free to clone, edit, or submit a pull request.</p>
          </div>
          <div class="col-sm-4 offset-md-1 py-4">
            <ul class="list-unstyled">
              <li><a href="https://twitter.com/anthilemoon" class="text-white">Twitter</a></li>
              <li><a href="https://github.com/anthilemoon" class="text-white">GitHub</a></li>
              <li><a href="https://anne-laure.net/" class="text-white">Website</a></li>
            </ul>
          </div>
        </div>
      </div>
    </div>
    <div class="navbar navbar-dark bg-blue shadow-sm">
      <div class="container d-flex justify-content-between">
        <a href="/" class="navbar-brand d-flex align-items-center title">
          <strong>Pixel Progress</strong>
        </a>
        <button class="navbar-toggler collapsed" type="button" data-toggle="collapse" data-target="#navbarHeader" aria-controls="navbarHeader" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
      </div>
    </div>
  </header>

  
        <div class="container-fluid"></div>
            <div class="graph">
                <ul class="months">
                  <li>Jan</li>
                  <li>Feb</li>
                  <li>Mar</li>
                  <li>Apr</li>
                  <li>May</li>
                  <li>Jun</li>
                  <li>Jul</li>
                  <li>Aug</li>
                  <li>Sep</li>
                  <li>Oct</li>
                  <li>Nov</li>
                  <li>Dec</li>
                </ul>
                <ul class="days">
                  <li>Sun</li>
                  <li>Mon</li>
                  <li>Tue</li>
                  <li>Wed</li>
                  <li>Thu</li>
                  <li>Fri</li>
                  <li>Sat</li>
                </ul>
                <ul class="squares">
              
                    <% for (var i = 0; i < 364; i++) { %>
              
                      <li data-toggle="tooltip" data-placement="bottom" data-animation="false" delay="0" title="<%= data[1][i] %>" data-level="<%= data[3][i] %>"></li>
                    
                      <% } %>
              
                </ul>
              </div>
        </div>

<% include partials/footer %>